    "ingo-deep-defaults": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-plugin-promise": "^3.8.0",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.4",
